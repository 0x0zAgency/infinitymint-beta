{
    "name": "infinitymint",
    "description": "InfinityMint - Powered by Hardhat and Ganache is a web3 based NFT minting toolset that allows users to mint NFTs on the Ethereum blockchain. Developed by 0x0z Agency.",
    "homepage": "https://github.com/0x0zAgency/infinitymint-beta#readme",
    "version": "1.10.1",
    "main": "dist/core.js",
    "bin": "./main.js",
    "directories": {
        "test": "tests"
    },
    "_moduleAliases": {
        "@app": "dist/app/",
        "@typechain-types": "dist/typechain/"
    },
    "scripts": {
        "test": "npx hardhat test",
        "build": "rm -R -f ./dist && npx tsc",
        "nix": "export NIXPKGS_ALLOW_UNFREE=1 && nix develop",
        "start-dev": "npx hardhat compile && npx tsc && npx ts-node core.ts",
        "ganache": "npx ts-node run.ts --start-ganache --stay-open --start-express 'false' --show-all-logs",
        "server": "ts-node run.ts --start-ganache 'false' --start-express  --stay-open",
        "start": "npx ts-node console.ts",
        "panic": "killall node",
        "doc": "npx typedoc --options ./typedoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/0x0zAgency/infinitymint-beta.git"
    },
    "author": "Llydia Cross",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/0x0zAgency/infinitymint-beta/issues"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "standard": "^17.0.0",
        "ts-node": "^10.9.1",
        "ts-standard": "^12.0.2",
        "typechain": "^8.1.1",
        "typedoc": "^0.23.22",
        "typedoc-theme-hierarchy": "^3.0.2",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
        "@nomicfoundation/hardhat-toolbox": "^2.0.0",
        "@nomiclabs/hardhat-ethers": "^2.2.1",
        "@nomiclabs/hardhat-etherscan": "^3.1.3",
        "@openzeppelin/contracts": "^4.8.0",
        "bip39": "^3.0.4",
        "blessed": "^0.1.81",
        "blessed-contrib": "^4.11.0",
        "chai": "^4.3.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-remove-route": "^1.0.0",
        "fs-extra": "^11.1.0",
        "ganache": "^7.7.1",
        "glob": "^8.0.3",
        "hardhat": "^2.12.4",
        "hardhat-change-network": "^0.0.7",
        "hardhat-deploy": "^0.11.22",
        "helmet": "^6.0.1",
        "infinitymint": "^1.7.0",
        "ipfs-bitswap": "^15.0.0",
        "ipfs-core": "0.14.0",
        "ipfs-core-types": "^0.13.0",
        "jszip": "^3.10.1",
        "module-alias": "^2.2.2",
        "multiformats": "^9.6.5",
        "play-sound": "^1.1.5",
        "serialize-javascript": "^6.0.1",
        "sharp": "^0.31.3",
        "solc": "^0.8.12",
        "sqlite": "^4.1.2",
        "sqlite3": "^5.1.4",
        "tcp-ping-port": "^1.0.1",
        "telnet2": "^0.0.1",
        "underscore": "^1.13.6",
        "uuid": "^9.0.0",
        "web3.storage": "^4.4.0",
        "yargs": "^17.6.2"
    }
}
